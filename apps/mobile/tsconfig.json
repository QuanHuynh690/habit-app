{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
   "baseUrl": ".", 
    "paths": {
      "@/*": ["*"],
      "@services/*": ["../../services/*"],
      "@shared/*": ["../../shared/*"],
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "env.d.ts",
  ]
}
